<html>
  <script src="/lib/jquery-3.6.0.min.js"></script>

<style>
  @font-face {
    font-family: "nanumB";
    src: url("/res/font/NanumSquareB.ttf");
  }
  body{     
   background-color: rgb(36, 36, 36);
   color:white;

   margin: 0;
    margin-bottom: 20px;
  font-family: 'nanumB';
  text-align:center;

  }
  a,p{
    font-family: 'nanumB';
  }
  .filter-invert{
    filter: invert();
  }
  
  #navbar{
    /* position:fixed; */
    display: flex;
    /* background: none; */
    justify-content:flex-start;
    flex-direction: row-reverse;
    align-items: stretch;
    height: 40px;
    background-color: rgb(36, 36, 36);
    position: sticky;
    top: 0;
    padding: 5px;
    border-bottom: 1px solid rgb(95, 95, 95);
    z-index: 3;
  }
  #navbar img, #navbar > div{
    color:white;
    /* background-color: #7E00BF; */
    background: #505060;
    line-height: 30px;

    cursor:pointer;
    text-align:center;
    font-size: 20px;
    border:none;
    vertical-align: middle;
    border-radius: 20%;
    padding: 5px;
    margin-right: 5px;
    height: 30px;
  }
  #navbar img:hover,#navbar > div:hover{
    background-color: #7a7a92;
  }
  #navbar img.delete_post{
    /* background-color: #ff5656; */
  }
  .postimg{
    max-width: 40%;
    max-height: 200px;
  }
  #post_container{
    width: min(95%,900px);
    display: inline-block;
    background-color: rgb(65, 65, 65);
    padding: 10px;
    text-align: start;
  }
  .title_container{
    text-align: start;
  }
  .title{
    font-size: 40px;
    text-align: center;
  }
  .info_container{
    text-align: start;
  }
  .info{
    font-size: 10px;
    color: #b2b2b2;
  }
  .content{
    margin-bottom:100px ;
  }
</style>
<head>
  <meta name="viewport" content="initial-scale=1.0, maximum-scale=1.2, user-scalable=no" />

  <title>Not found</title>
  <link rel="icon" type="image/x-icon" href="/res/img/ui/favicon.png">
  <meta charset="utf-8" >
</head>

<body>
 
  <div id="navbar">
    <!-- <img src="/res/img/svg/home.svg" id="homebtn"  onclick="golink('/')"><br> -->
    <img src="/res/img/svg/home.svg" id="homebtn">
  </div>
 <div id="post_container">


  <div class="title_container">
    <%if(status===404){%>
      <a class="title"> 404 NOT FOUND! </a>
    <%}else if(status===500){%>
      <a class="title"> 500 Internal Server Error! </a>
    <%}else {%>
      <a class="title">Error!</a>
    <%}%>
    
  </div>
      
  <hr>

     
 </div>
   
</body>
<script>
$("#homebtn").click(()=>{
    window.location.href="/index.html"
})

</script>
</html>